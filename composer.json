{
    "name": "qbejs/laravel-dto-mapper",
    "description": "Automatic DTO mapping for Laravel using PHP 8 Attributes",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "dto",
        "mapper",
        "attributes",
        "validation"
    ],
    "authors": [
        {
            "name": "Jakub Skowron",
            "email": "jakub@skowron.dev"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/validation": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelDtoMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelDtoMapper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelDtoMapper\\Providers\\DtoMapperServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
